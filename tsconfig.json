{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": ["ESNext", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "noEmit": false,
    "rootDir": "./src",
    "outDir": "./public"
  },
  "include": ["src", "src/types"],
  "exclude": ["node_modules"]
}
